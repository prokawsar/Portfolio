		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"postcss": "^8.4.16",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "^5.1.10",
		"tailwindcss": "^3.4.14",
		"tslib": "^2.3.1",
		"typescript": "^5.6.3",
		"vite": "^5.4.10"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-vercel": "^5.4.6",
